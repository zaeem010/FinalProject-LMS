@model IEnumerable<FinalProject_LMS.ViewModels.UMessageView>
<div class=" jumbotron">
    @{
        ViewBag.Title = "OutIndex";
    }

    <h2>Outbox</h2>

    <p>
        @Html.ActionLink("Send A Message", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.Label("To")
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rubrik)
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>


                <td>
                    @Html.DisplayFor(modelItem => item.ReciverName)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Rubrik)
                </td>



                <td>

                    @Html.ActionLink("Read", "Details", new { id = item.Id })

                </td>
            </tr>
        }

    </table>
</div>
@section Scripts {

    @Scripts.Render("~/bundles/message")
}


